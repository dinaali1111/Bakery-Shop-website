{
  "name": "dina-business-platform",
  "version": "1.0.0",
  "description": "Modern business platform with admin dashboard, built with Next.js 14, TypeScript, and Prisma",
  "author": {
    "name": "Dina Ali",
    "email": "dinaali1111@gmail.com",
    "url": "https://github.com/dinaali1111"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "prisma",
    "business",
    "dashboard",
    "admin",
    "tailwindcss",
    "authentication",
    "cms",
    "responsive",
    "modern",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dinaali1111/dina-business-platform.git"
  },
  "bugs": {
    "url": "https://github.com/dinaali1111/dina-business-platform/issues"
  },
  "homepage": "https://dina-business.vercel.app",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "postinstall": "prisma generate",
    "deploy": "npm run build && npm run start",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^4.29.9",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^10.16.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "14.0.0",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "jest": "^29.7.0",
    "playwright": "^1.44.1",
    "prisma": "^5.22.0",
    "typescript": "^5.3.3"
  }
}
